{
  "name": "truck-queue",
  "version": "1.1.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.mycompany.myapp",
    "productName": "Truck-Queue-App",
    "files": [
      "main.js",
      "package.json",
      "src/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac":null,
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.20"
  }
}
